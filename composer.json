{
    "name": "thelia/smarty-module",
    "description": "Smarty template engine integration for Thelia ",
    "type": "thelia-module",
    "license": "LGPL-3.0+",
    "homepage": "http://thelia.net/",
    "support": {
        "forum": "http://thelia.net/forum",
        "wiki": "http://doc.thelia.net"
    },
    "require": {
        "thelia/installer": "~1.0",
        "smarty/smarty": "v3.1.19"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "smarty/smarty",
                "version": "3.1.19",
                "dist": {
                    "url": "http://www.smarty.net/files/Smarty-3.1.19.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "http://smarty-php.googlecode.com/svn/",
                    "type": "svn",
                    "reference": "tags/Smarty_3_1_19/distribution/"
                },
                "autoload": {
                    "classmap": [
                        "libs/"
                    ]
                }
            }
        }
    ],
    "extra": {
        "installer-name": "TheliaSmarty",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
